<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dev.anggur.sistempakaranggur">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Sistem Pakar Anggur"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.DiagnosaActivity"
            android:label="Daftar Diagnosa" />
        <activity
            android:name=".activity.LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.RegisterActivity"
            android:label="Register" />
        <activity
            android:name=".activity.KonsultasiActivity"
            android:label="Konsultasi" />
        <activity android:name=".activity.DetailDiagnosaActivity"
            android:label="Detail Diagnosa"/>
        <activity
            android:name=".activity.TambahGejalaToDiagnosaActivity"
            android:label="Tambah Gejala Diagnosa" />
        <activity android:name=".activity.TambahDiagnosaActivity" />
        <activity android:name=".activity.MenuActivity"/>
    </application>

</manifest>